data = {"reviewDetails":{"startIndex":0,"numberOfReviewsInThisPage":20,"reviewSummaryCollection":{"reviewSummary":[{"hotelId":"234","totalReviewCnt":1307,"avgOverallRating":3.6052027,"cleanliness":3.7712314,"serviceAndStaff":3.895177,"roomComfort":3.5348117,"hotelCondition":3.1713834,"convenienceOfLocation":4.580809,"neighborhoodSatisfaction":4.3080807,"roomQuality":0,"targetedBrand":"Expedia","originSummary":[{"origin":"Expedia","reviewCnt":1307,"recommendedPercent":80.56618,"avgOverallRating":3.6052027,"cleanliness":3.7712314,"serviceAndStaff":3.895177,"roomComfort":3.5348117,"hotelCondition":3.1713834,"convenienceOfLocation":4.580809,"neighborhoodSatisfaction":4.3080807,"valueForMoney":0,"roomQuality":0,"categoryCounts":{"Everyone":471,"Couples":91,"BusinessTravelers":5,"Students":2,"Families":58,"Other":13},"languageCounts":{"en":1307}}],"recommendedPercent":80.56618,"valueForMoney":0,"categoryCounts":{"Everyone":471,"Couples":91,"BusinessTravelers":5,"Students":2,"Families":58,"Other":13},"languageCounts":{"en":1307},"featuredReview":{}}]},"reviewCollection":{"review":[{"title":"Great Location","reviewText":"We were surprised by our upgrade to a Jr. Suite.  The room was clean and the view was fabulous!  It was a pleasant surprise to have a toaster and a few kitchen items.  The bed was comfortable and a nice balcony to enjoy the view while we had our dinner and a glass of wine.","positiveRemarks":"We enjoyed the proximity to the beach and shops and restaurants.  The staff was friendly and helpful.","negativeRemarks":"It would be nice to have beach chairs and umbrellas available at the hotel as some of the other hotels do.","locationRemarks":"Close to places like Leonard's Bakery and Rainbow Drive-In.  A short walk to Duke's for music and a cocktail and easy walk to rent surfboards and SUPs","tpid":"1","eapid":"0","langId":"1033","reviewId":"56da08b7bae22b09968a99d0","brandType":"Expedia","photos":[],"managementResponses":[],"moderationStatus":"APPROVED","hotelId